{
  "name": "domco-vue-elysia",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "i": "rimraf dist && rimraf node_modules &&  rimraf bun.lock ",
    "dev": "bunx --bun  vite",
    "check": "bunx --bun  tsc",
    "build": "bunx --bun vite build",
    "preview": " bunx --bun  vite preview",
    "format": " bunx biome check --write",
    "docker:dev": "docker-compose -f  .container/dev/docker-compose.yml --env-file .env.development up -d",
    "schema-typebox-gen": "bunx schema-typebox-gen src/server/src/db",
    "db:push": "bunx drizzle-kit push --config=.container/dev/drizzle-dev.config.ts",
    "db:comments": "bun run src/server/src/db/comment/comments-config.ts"
  },
  "dependencies": {
    "@elysiajs/eden": "^1.3.3",
    "@elysiajs/swagger": "^1.3.1",
    "drizzle-orm": "^0.44.5",
    "drizzle-typebox": "^0.3.3",
    "elysia": "^1.3.21",
    "logixlysia": "^5.1.1",
    "pg": "^8.16.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "@primevue/themes": "^4.3.8",
    "primevue": "4.3.8"
  },
  "devDependencies": {
    "@domcojs/vercel": "^3.0.0",
    "@pori15/schema-typebox-generator": "^2.0.6",
    "@primevue/auto-import-resolver": "^4.3.8",
    "@robino/tsconfig": "^1.1.0",
    "@types/bun": "1.2.21",
    "@types/pg": "^8.15.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "domco": "^4.3.0",
    "drizzle-kit": "^0.31.4",
    "rollup-plugin-visualizer": "^6.0.3",
    "ts-morph": "^26.0.0",
    "typescript": "^5.9.2",
    "unocss": "^66.5.0",
    "unocss-preset-animations": "^1.2.1",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^7.1.4",
    "@biomejs/biome": "2.2.2",
    "bun-types": "^1.2.21",
    "dotenv": "^17.2.2",
    "tsx": "^4.20.5"
  },
  "overrides": {
    "@sinclair/typebox": "0.34.33"
  }
}